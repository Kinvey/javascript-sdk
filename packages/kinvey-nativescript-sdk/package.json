{
  "name": "kinvey-nativescript-sdk",
  "description": "Kinvey NativeScript SDK for developing NativeScript applications.",
  "version": "3.13.0-next.6",
  "homepage": "https://www.progress.com/kinvey",
  "license": "Apache-2.0",
  "contributors": [
    "Thomas Conner (https://github.com/thomasconner)"
  ],
  "main": "src/index.js",
  "scripts": {
    "postinstall": "node lib/postinstall.js",
    "preuninstall": "node lib/preuninstall.js"
  },
  "dependencies": {
    "nativescript-hook": "0.2.4"
  },
  "peerDependencies": {
    "@babel/runtime": "~7.2.0",
    "@angular/core": "~7.2.2",
    "events": "~3.0.0",
    "js-base64": "~2.5.0",
    "lodash": "~4.17.11",
    "loglevel": "~1.6.1",
    "loglevel-plugin-prefix": "~0.8.4",
    "lokijs": "~1.5.6",
    "nativescript-secure-storage": "~2.3.0",
    "nativescript-sqlite": "~2.2.3",
    "nativescript-urlhandler": "~1.2.2",
    "p-queue": "~3.0.0",
    "pubnub": "~4.21.7",
    "rxjs": "~6.3.3",
    "sift": "~7.0.1",
    "tns-core-modules": "~5.1.1",
    "url": "~0.11.0",
    "url-join": "~4.0.0"
  },
  "files": [
    "lib/**/*",
    "platforms/**/*",
    "src/**/*",
    "angular.js"
  ],
  "nativescript": {
    "hooks": [
      {
        "type": "before-checkForChanges",
        "script": "lib/before-checkForChanges.js",
        "inject": true
      },
      {
        "type": "before-preview-sync",
        "script": "lib/before-preview-sync.js",
        "inject": true
      }
    ],
    "platforms": {
      "ios": "5.1.0",
      "android": "5.1.0"
    }
  }
}
